FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0092;

boundaryField
{
    concreteWall
    {
        type            nutkWallFunction; //nutkRoughWallFunction
        value           uniform 0.0092;
    }
    steelWall_us
    {
        type            nutkWallFunction;
        value           uniform 0.0092;
    }
    steelWall_ds
    {
        type            nutkWallFunction;
        value           uniform 0.0092;
    }
    ".*"
    {
        type            calculated;
        value           uniform 0;
    }

}


